# Database (ใช้ Turso - SQLite serverless ฟรี)
DATABASE_URL=libsql://darkwebsite-db.turso.io
DATABASE_AUTH_TOKEN=generated_token_here

# Supabase (สำหรับ Authentication)
SUPABASE_URL=https://darkwebsite.supabase.co
SUPABASE_SERVICE_KEY=generated_service_key_here
SUPABASE_ANON_KEY=generated_anon_key_here

# JWT & Security
JWT_SECRET=$(openssl rand -base64 32)
COOKIE_SECRET=$(openssl rand -base64 32)

# App Config
VITE_APP_ID=darkwebsite
NODE_ENV=production
PORT=3000

# API Base URL
VITE_API_URL=https://darkwebsite.vercel.app/api
